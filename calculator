def add (x, y):
 return x + y
def subtract (x, y):
 return x - y
def multiply(x, y):
 return x * y
def divide(x,y):
return x / y
  # Ask user for numbers
  a = float(input("Enter first number: "))
  b = float(input("Enter second number: "))
  print("Add:", add(a,b))
  print("Subtract:", subtract(a,b))
  print("Multiply:", multiply(a,b))
  print("Divide:", divide(a,b))

